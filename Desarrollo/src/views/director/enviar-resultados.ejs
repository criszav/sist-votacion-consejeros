            <h2>Detalles de la Votación</h2>
                <div class="seguimiento-list">
                    <ul class="item-list">
                        <% for (consejero of consejeros) { %>
                            <li class="<%= consejero.id%>">
                                <div class="persona-seg">
                                    <div class="avatar-seg">
                                        <img src="<%= consejero.url_img %>" alt="Avatar">
                                    </div>

                                    <div class="seg-info">
                                        <h3>
                                            <%= consejero.nombre_votacion %>
                                        </h3>
                                        <p>
                                            <%= consejero.nombre %>
                                                <%= consejero.apellido %>
                                        </p>
                                    </div>
                                    <div class="cant-votos">
                                        <%= consejero.votos %> voto(s) 
                                    </div>
                                </div>
                            </li>
                            <% } %>
                    </ul>
